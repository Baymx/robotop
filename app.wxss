/**app.wxss**/
.a{
  color: #017FF7;
}
.nav{
  height: 98rpx;
  width: 100%;
  background: #ffffff;
  display: flex;
  position:fixed;
  bottom:0;
  z-index: 999;
  border-top:1px solid #E4E4E4 ;
  word-break: normal !important; 
  overflow: visible !important;
}
.navBar{
  width: 20%;
  height: 100%;
  text-align: center;
  font-size: 20rpx;
  padding-top: 15rpx;
  word-break: normal !important; 
  overflow: visible !important;
}
.navImg1{
  width: 51rpx;
  height: 43rpx;
  margin: auto;
  margin-bottom: 6rpx;
}
.navImg2{
  width: 37rpx;
  height: 40rpx;
  margin: auto;  
  margin-bottom: 6rpx;
}
.navImg3{
  width: 85rpx;
  height: 85rpx;
  margin: auto;
  border-top-left-radius: 50%;
  border-top-right-radius: 50%;
  border-top: 1px solid #E4E4E4;
  background-color: #ffffff;
}
.navImg4{
  width: 43rpx;
  height: 40rpx;
  margin: auto;  
  margin-bottom: 6rpx;
}
.navImg5{
  width: 40rpx;
  height: 40rpx;
  margin: auto; 
  margin-bottom: 6rpx; 
}
.s{
  position: relative;
  top:-40rpx;
  word-break: normal !important; 
  overflow: visible !important;
}
image{
  width: 100%;
  height: 100%;
}
.clearfix:after {
  content: "";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}