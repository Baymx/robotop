page {
  background: #f5f5f5;
}
.nav{
  height: 98rpx;
  width: 100%;
  background: #ffffff;
  display: flex;
  position:fixed;
  bottom:0;
  z-index: 999;
  border-top:1px solid #E4E4E4 ;
  word-break: normal !important; 
  overflow: visible !important;
}
.navBar{
  width: 20%;
  height: 100%;
  text-align: center;
  font-size: 20rpx;
  padding-top: 15rpx;
  word-break: normal !important; 
  overflow: visible !important;
}
.navImg1{
  width: 51rpx;
  height: 43rpx;
  margin: auto;
}
.navImg2{
  width: 37rpx;
  height: 40rpx;
  margin: auto;  
}
.navImg3{
  width: 85rpx;
  height: 85rpx;
  margin: auto;
  border-top-left-radius: 50%;
  border-top-right-radius: 50%;
  border-top: 1px solid #E4E4E4;
  background-color: #ffffff;
}
.navImg4{
  width: 43rpx;
  height: 40rpx;
  margin: auto;  
}
.navImg5{
  width: 40rpx;
  height: 40rpx;
  margin: auto;  
}
.s{
  position: relative;
  top:-40rpx;
  word-break: normal !important; 
  overflow: visible !important;
}
image{
  width: 100%;
}
.clearfix:after {
  content: "";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.tab-h {
  font-size: 30rpx;
  height: 100rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 100rpx;
  background: #fff;
  border-top: 1rpx solid #dcdcdc;
  box-shadow: 0 2px 10px #eee;
}

.tab-item {
  width: 20%;
  text-align: center;
  display: inline-block;
  color: #000;
}

.tab-item.active {
  color: #017ff7;
  position: relative;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 6rpx;
  width: 60rpx;
  background: #017ff7;
  position: absolute;
  bottom: 1rpx;
  left: 45rpx;
}

.scoll-h {
  height: 100%;
  width: 100%;
  box-sizing: border-box;
}

.ht-box {
  background: #fff;
  padding: 20rpx;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}

.ht-box-tit {
  height: 88rpx;
  width: 100%;
  border-bottom: 1rpx solid #f5f5f5;
}

.ht-box-tit image {
  width: 70rpx;
  height: 70rpx;
  display: inline-block;
}

.ht-box-tit text {
  font-size: 32rpx;
  display: inline-block;
  height: 87rpx;
  line-height: 87rpx;
  position: relative;
  top: -22rpx;
  margin-left: 20rpx;
  color: #000;
  font-weight: 600;
}

.bt-box-group {
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
}

.bt-box-group:last-child {
  border-bottom: none;
}

.bt-box-group>image {
  width: 250rpx;
  height: 160rpx;
  display: block;
  float: left;
}

.bt-box-rt-wz {
  width: 420rpx;
  height: 160rpx;
  font-size: 32rpx;
  float: left;
  position: relative;
}

.bt-box-rt-wz .tt {
  margin-left: 20rpx;
  font-size: 36rpx;
  padding: 10rpx 0;
  display: block;
  width: 350rpx;
  height: 36rpx;
  overflow: hidden;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}

.bt-box-rt-wz .address {
  color: #666;
  margin-left: 20rpx;
  font-size: 28rpx;
  width: 320rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-top: 30rpx;
}

.address-img {
  display: block;
  position: absolute;
  right: 0;
  bottom: 0;
  width: 78rpx;
  height: 78rpx;
}

.zby-address {
  font-size: 30rpx;
  width: 100%;
  color: #666;
  display: block;
  float: left;
  height: 70rpx;
  line-height: 70rpx;
  position: relative;
  margin-top: 25rpx;
}

.zby-address text {
  display: block;
  width: 550rpx;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
